#include "palindrome.h"

int is_palindrome(char *str)
{
    char *reverse = malloc(sizeof(my_strlen(str)));
//    reverse = my_revstr(str);
    reverse = "237";
    
    printf("rev = %s\n", reverse);
    if (my_strcmp(str, reverse) == 0)
        //      printf("palindrome\n");
        return (PALINDROME);
    else 
        // printf("not palindrome\n");
        return (NOT_PALINDROME);
//    return 0;
}
